.bla-hm {
    cursor: pointer;
    background-size: contain;
    background-repeat: no-repeat;
    display: inline-block;
    width: 80px;
    height: 60px;
    -webkit-transition: all 100ms ease-in;
    -moz-transition: all 100ms ease-in;
    transition: all 100ms ease-in;
    -webkit-filter: brightness(1.8) opacity(.6);
    -moz-filter: brightness(1.8) opacity(.6);
    filter: brightness(1.8) opacity(.6);
}

.bla-hm:hover {
    -webkit-filter: brightness(1.2) grayscale(.5) opacity(.9);
    -moz-filter: brightness(1.2) grayscale(.5) opacity(.9);
    filter: brightness(1.2) grayscale(.5) opacity(.9);
}

.bla-map {
    cursor: pointer;
    background-size: contain;
    background-repeat: no-repeat;
    display: inline-block;
    width: 150px;
    height: 80px;
    -webkit-transition: all 100ms ease-in;
    -moz-transition: all 100ms ease-in;
    transition: all 100ms ease-in;
    -webkit-filter: brightness(1.8) opacity(.6);
    -moz-filter: brightness(1.8) opacity(.6);
    filter: brightness(1.8) opacity(.6);
}

.bla-map:hover {
    -webkit-filter: brightness(1.2) grayscale(.5) opacity(.9);
    -moz-filter: brightness(1.2) grayscale(.5) opacity(.9);
    filter: brightness(1.2) grayscale(.5) opacity(.9);
}


/* Extras */

a {
    color: #444;
    text-decoration: none;
}

p {
    margin-bottom: .3em;
}

.matchselector input:active+.bla-hm {
    opacity: .9;
}

.matchselector input:checked+.bla-hm {
    -webkit-filter: none;
    -moz-filter: none;
    filter: none;
}

.matchselector input:active+.bla-map {
    opacity: .9;
}

.matchselector input:checked+.bla-map {
    -webkit-filter: none;
    -moz-filter: none;
    filter: none;
}

